entity ADDRESS {
    key ADDRESS_ID : Integer generated by default as identity;
        LINE1      : hana.VARCHAR(45) null;
        LINE2      : hana.VARCHAR(45) null;
        LINE3      : hana.VARCHAR(45) null;
        LINE4      : hana.VARCHAR(45) null;
        LOCALITY   : hana.VARCHAR(45) null;
        REGION     : hana.VARCHAR(45) null;
        POSTCODE   : hana.VARCHAR(45) null;
        COUNTRY    : hana.VARCHAR(45) null;
};