entity COMMENT {
    key COMMENT_ID     : Integer generated by default as identity;
        CLASSIFICATION : hana.VARCHAR(255) null;
        CAMKEY         : hana.VARCHAR(45) null;
        CREATED_DATE   : UTCTimestamp null;
        CREATED_BY     : hana.VARCHAR(45) null;
        MODIFIED_DATE  : UTCTimestamp null;
        MODIFIED_BY    : hana.VARCHAR(45) null;
        COMMENT        : hana.VARCHAR(45) null;
};